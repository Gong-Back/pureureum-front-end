{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "printWidth": 80,
  "arrowParens": "always",
  "useTabs": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "quoteProps": "as-needed",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(next/(.*)$)|^(next$)|^(react/(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "^[@/$]",
    "^[./]"
  ]
}
